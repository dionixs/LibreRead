= form_with url: import_notes_path, method: :get do |f|
  .flex.justify-between
    = f.text_field :tags, id: 'search-by-tags', placeholder: t('notes.index.form.search.placeholder'), required: true, class: 'w-full', autocomplete: "off", autofocus: ""
    = f.submit t('notes.index.form.search.btn'), class: "text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-4 py-2 dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800"
